#include <iostream>
#include <cstdlib>
#include <map>
#include <vector>
#include <algorithm>
#include <fstream>
#include <cstring>
using namespace std;
int main()
{
	vector<int> a;
	int n;
	cout<<"Upisite neke brojeve i zavrsite s '0'\n";
	cin>>n;
	int b=0;
	while(n!=0)
		{
			a.push_back(n);	
			cin>>n;
		}
	cout<<"\n Prije sortiranja:\n";
	for(int i=0;i<a.size();i++)
	{
		cout<<endl<<a.at(i);
	}
	cout<<"\n Poslje sortiranja \n";
	sort(a.begin(), a.end());	
	
	for(int i=0;i<a.size();i++)
	{
		cout<<endl<<a[i];
	}	
	
}
