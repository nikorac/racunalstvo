#include <iostream>
#include <cstdlib>
#include <fstream>
using namespace std;
int main()
{
	string a;
	string b;
	string c;
	cout<<"Vaše ime";
	cin>>a;
	cout<<"vase prezime";
	cin>>b;
	cout<<"Vaša dob";
	cin>>c;
	ofstream ispis("D:\\nekerijeci.txt");
	ispis<<a<<b<<c;
	ispis.close();
	system("PAUSE");
	return 0;
}
