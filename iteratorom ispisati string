  
#include <iostream>
#include <cstdlib>
#include <map>
#include <vector>
#include <algorithm>
#include <fstream>
#include <cstring>
using namespace std;
int main()
{
	string b;
	getline(cin,b);
	int n=1;
	cout<<endl;
	for(string::iterator it=b.begin(); it!=b.end();it++)
	{
		cout<<*it;
		if(isspace(*it))
		{
			n++;
			cout<<endl;
		}
	}
	cout<<endl<<endl<<"Recenica ima "<<n<<" rijeci.";

}
