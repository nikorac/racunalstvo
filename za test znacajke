istaknuti jednu stvar iz map<>
std::map<int , std::string>::iterator it = vozila.find(2);

std::cout<<"rijec "<<it->second<<" je pronadena"<<endl;

napravit map<>
std::map<int , std::string> vozila;

vozila[1] = "auto";

vozila[2] = "brod";

vozila[3] = "motor";

vozila[4] = "avion";

zamijeniti varijablu u map<>
std::map<int , std::string>::iterator it = vozila.find(1);

vozila.erase(it);

vozila.insert(std::pair<int , std::string>(1, "trajekt"));

isdigit()
isalpha()
