#include <iostream>
#include <cstdlib>
#include <map>
#include <vector>
#include <algorithm>
#include <fstream>
#include <cstring>
using namespace std;
int main()
{
	string word;
	string max;
	cout<<"Upisite rečenicu pa stisnite 'Ctrl+z'\n";
	cin>>word;
	while(cin>>word)
	{
		if(word.size()>max.size())
		max=word;
	}
	cout<<max;
}
